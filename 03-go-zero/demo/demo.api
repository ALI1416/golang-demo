syntax = "v1"

type Request {
	Name string `path:"name,options=you|me"`
}

type Response {
	Message string `json:"message"`
}

type ListItem {
	Value string `json:"value"`
}

service demo-api {
	@handler DemoHandler
	get /from/:name (Request) returns (Response)

	@handler ListHandler
	get /list/get returns ([]ListItem)
}

